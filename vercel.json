{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "functions": {
        "*.js": {
            "maxDuration": 15
        },
        "/*.js": {
            "maxDuration": 15
        }
    }
}